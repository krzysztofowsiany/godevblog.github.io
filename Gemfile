source 'https://rubygems.org'
gem 'github-pages', group: :jekyll_plugins


group :jekyll_plugins do
    gem 'jekyll', '3.7.3'
    gem "jekyll-feed"
    gem "jekyll-youtube"
    gem 'jekyll-sitemap'
    gem 'jekyll-twitter-plugin'
    gem 'jekyll-paginate'
    gem 'jekyll-seo-tag'

    gem 'jekyll-tagging', :git => 'https://github.com/pattex/jekyll-tagging.git'
 end

